rt Layout from \"./components/layout/Layout\";\n// import Quotes from \"./pages/Quotes\";\n// import AddQuote from \"./pages/AddQuote\";\n// import QuoteDetail from \"./pages/QuoteDetail\";\n// import NotFound from \"./pages/NotFound\";\nimport LoadingSpinner from \"./components/UI/LoadingSpinner\";\n\nconst Quotes = React.lazy(()=> import(\"./pages/Quotes\"))\nconst AddQuote = React.lazy(()=> import(\"./pages/AddQuote\"))\nconst QuoteDetail = React.lazy(()=> import(\"./pages/QuoteDetail\"))\nconst NotFound = React.lazy(()=> import(\"./pages/NotFound\"))\n\nfunction App() {\n  return (\n  <>\n    <Layout>\n      <Switch>\n        <Suspense fallback={<div className=\"centered\"><LoadingSpinner/></div>}>\n          <Route path='/' exact>\n            <Redirect to='/quotes'/>\n          </Route>\n          <Route path='/quotes' exact>\n            <Quotes/>\n          </Route>\n          <Route path='/quotes/:quoteId'>\n            <QuoteDetail/>\n          </Route>\n          <Route path='/add-quote'>\n            <AddQuote/>\n          </Route>\n          {/* <Route path='*'  >\n            <NotFound/>\n          </Route> */}\n        </Suspense>\n      </Switch>\n      </Layout>\n  </>\n  );\n}\n\nexport default App;\n","/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/layout/MainHeader.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/pages/Quotes.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/pages/AddQuote.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/quotes/QuoteList.js",["93"],"import { Fragment } from 'react';\nimport { useHistory , useLocation , useRouteMatch} from 'react-router-dom';\nimport QuoteItem from './QuoteItem';\nimport classes from './QuoteList.module.css';\n\nconst sortQuotes = (quotes , ascending)=>{\n  return quotes.sort((quoteA , quoteB)=>{\n    if(ascending){\n      return quoteA.id > quoteB.id ? 1 : -1\n    } else {\n      return quoteA.id < quoteB.id ? 1 : -1\n    }\n  })\n}\n\nconst QuoteList = (props) => {\n  const history = useHistory()\n  const location = useLocation()\n  const queryParams = new URLSearchParams(location.search)\n  console.log(location);\n  const isSortingAscending = queryParams.get('sort') === 'asc'\n\n  const sortedQuotes = sortQuotes(props.quotes , isSortingAscending)\n  const changeSortingHandler = ()=>{\n    history.push({\n      pathname: location.pathname ,\n      search: `?sort=${isSortingAscending ? 'desc' : 'asc'}`\n    })\n    \n  }\n  return (\n    <Fragment>\n      \n      <div className={classes.sorting}>\n        <button onClick={changeSortingHandler}>Sorting {isSortingAscending ? 'Descending' : 'Ascending'}</button>\n      </div>\n      <ul className={classes.list}>\n        {sortedQuotes.map((quote) => (\n          <QuoteItem\n            key={quote.id}\n            id={quote.id}\n            author={quote.author}\n            text={quote.text}\n          />\n        ))}\n      </ul>\n    </Fragment>\n  );\n};\n\nexport default QuoteList;\n","/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/quotes/QuoteItem.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/quotes/QuoteForm.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/quotes/NoQuotesFound.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/pages/QuoteDetail.js",["94"],"import React from 'react'\nimport { useParams , Route , Link , useRouteMatch} from 'react-router-dom'\nimport Comments from '../components/comments/Comments'\nimport HighlightedQuote from '../components/quotes/HighlightedQuote'\nimport useHttp from '../hooks/use-http'\nimport { getSingleQuote } from '../lib/api'\nimport { useEffect } from 'react'\nimport LoadingSpinner from '../components/UI/LoadingSpinner'\n\n// import NoQuotesFound fr\n\n\nconst DUMMY_QUOTES = [\n    {id:'q1', author:'Max',text:'Learning React is fun'},\n    {id:'q2', author:'Max',text:'Learning programming is fun'}\n]\n\nfunction QuoteDetail() {\n\n    const params = useParams()\n    const match = useRouteMatch()\n    const {quoteId} = params\n    const {sendRequest , status , data: loadedQuote , error} = useHttp(getSingleQuote , true)\n    useEffect(()=>{\n        sendRequest(quoteId)\n\n    },[sendRequest,quoteId])\n\n    if(status === 'pending'){\n        return(\n            <div className='centered'>\n                <LoadingSpinner/>\n            </div>\n        )\n\n    }\n    if(error){\n        return(\n            <p className='centered'>{error}</p>\n        )\n    }\n   \n    console.log(loadedQuote.text);\n\n    if(!loadedQuote.text){\n        return(\n            <p className='centered'> no quote found</p>\n        )\n    }\n    \n\n  return (\n    <React.Fragment>\n       <HighlightedQuote text= {loadedQuote.text} author= {loadedQuote.author}/>\n       <Route path={match.path} exact>\n        <div className={`centered`}>\n            <Link className='btn--flat' to={`${match.url}/comments`}>Load comments</Link>\n        </div>\n       </Route>\n\n       <Route path={`${match.path}/comments`}>\n            <Comments/>\n       </Route>\n\n    </React.Fragment>\n  )\n}\n\nexport default QuoteDetail","/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/comments/Comments.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/comments/NewCommentForm.js",[],"/Users/souhaybkamaldine/Desktop/programming/React/maximillian/09-time-to-practice-starting-code/src/components/layout/Layout.js",[],"/Users/souhaybkamaldine